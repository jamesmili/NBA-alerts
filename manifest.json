{
  "name": "NBA Alerts",
  "description": "Chrome alerts app about news surrounding the NBA",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["jquery.min.js"]
  },
  "browser_action":{
    "default_popup": "window.html"
  },
  "permissions": [
    "notifications",
    "tabs",
    "storage"
  ],
  "icons":{
    "32":"woj-bomb.png"
  }
}
