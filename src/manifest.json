{
  "name": "NBA News",
  "description": "A chrome extension that keeps you updated with any NBA related news.",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.min.js"],
    "persistent": false
  },
  "permissions": [
    "notifications",
    "tabs",
    "storage",
    "alarms"
  ],
  "icons":{
    "128":"nba.png"
  },
  "browser_action":{
    "default_popup": "index.html"
  }
}
